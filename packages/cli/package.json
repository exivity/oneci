{
  "name": "@openci/cli",
  "version": "0.0.0",
  "author": "Joram van den Boezem @hongaar",
  "bin": {
    "openci": "./bin/run"
  },
  "bugs": "https://github.com/exivity/openci/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@openci/api": "^0.0.0",
    "get-stdin": "^7.0.0",
    "js-yaml": "^3.13.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/js-yaml": "^3.12.1",
    "globby": "^8",
    "rimraf": "^2.6.3",
    "ts-node": "^7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/exivity/openci",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "openci",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "exivity/openci",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "yarn build",
    "start": "node ./bin/run",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "rimraf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
